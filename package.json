{
  "name": "oddcontrast",
  "version": "0.0.1",
  "license": "BSD-3-Clause",
  "engines": {
    "node": "16"
  },
  "scripts": {
    "dev": "svelte-kit dev --host 0.0.0.0",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "run-s prettier lint:js lint:sass check",
    "lint:js": "eslint --ignore-path .gitignore --fix .",
    "lint:js:ci": "eslint --ignore-path .gitignore .",
    "prettier": "prettier --ignore-path .gitignore --write .",
    "prettier:ci": "prettier --ignore-path .gitignore --check .",
    "lint:sass": "stylelint '**/*.scss' --fix",
    "lint:ci": "run-p prettier:ci lint:js:ci lint:sass:ci",
    "lint:sass:ci": "stylelint '**/*.scss'",
    "test": "echo 'TODO: write tests'"
  },
  "dependencies": {
    "accoutrement": "^4.0.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "sass": "^1.49.9",
    "sassdoc": "^2.7.3",
    "sassdoc-theme-herman": "^4.0.2",
    "stylelint": "^14.5.3",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-prettier": "^2.0.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.10.1",
    "tslib": "^2.3.1",
    "typescript": "~4.5.4"
  },
  "type": "module"
}
